{
  "name": "cypress-fakestore",
  "version": "1.0.0",
  "description": "Projeto de testes da Fake Store API com Cypress e Mochawesome",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run --spec 'cypress/e2e/**/*.cy.js'",
    "test": "npm run cy:run",
    "report:merge": "mochawesome-merge cypress/reports/.jsons/*.json -o cypress/reports/report.json",
    "report:html": "marge cypress/reports/report.json -f report -o cypress/reports/html",
    "posttest": "npm run report:merge && npm run report:html"
  },
  "author": "Evelyn Samara",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^14.5.4",
    "mochawesome": "^7.1.3",
    "cypress-mochawesome-reporter": "^3.2.0",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^6.2.0"
  }
}